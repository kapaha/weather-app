{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "rules": {
        "no-alert": 0,
        "no-console": 0,
        "no-use-before-define": ["error", {
            "functions": false
        }],
        "no-param-reassign": ["error", { "props": false }]
    }
}